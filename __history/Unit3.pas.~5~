    Panel2: TPanel;
    E_Nome: TEdit;
    E_Cognome: TEdit;
    Label3: TLabel;
    Label4: TLabel;
    Label9: TLabel;
    Label5: TLabel;
    E_Codice_Fiscale: TEdit;
    Label6: TLabel;
    ComboBox1: TComboBox;
    Label7: TLabel;
    E_Stato_Civile: TComboBox;
    Label8: TLabel;
    E_Indirizzo_di_Residenza: TEdit;
